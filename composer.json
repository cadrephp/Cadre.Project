{
    "name": "cadre/project",
    "type": "project",
    "require": {
        "php": ">=7.0",
        "atlas/orm": "^1.3.0",
        "cadre/atlasorm-debugbar-bridge": "^1.1.0",
        "cadre/cliadr": "^0.2.0",
        "cadre/domain-session": "^0.4.0",
        "cadre/module": "^0.5.0",
        "dflydev/fig-cookies": "^1.0.0",
        "maximebf/debugbar": "^1.13.1",
        "oscarotero/psr7-middlewares": "^3.20.0",
        "radar/adr": "^1.1.0",
        "relay/middleware": "^1.2.0",
        "robmorgan/phinx": "^0.8.1",
        "twig/twig": "^1.34.4",
        "zendframework/zend-diactoros": "^1.4.0"
    },
    "require-dev": {
        "atlas/cli": "^1.0.1",
        "pds/skeleton": "^1.0.0",
        "phing/phing": "^2.16.0",
        "phpunit/dbunit": "^3.0.0",
        "phpunit/phpunit": "^6.2.3",
        "squizlabs/php_codesniffer": "^3.0.1",
        "phpstan/phpstan": "^0.7.0"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "tests/",
            "PHP_CodeSniffer\\": "vendor/squizlabs/php_codesniffer/src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Shell",
            "email": "andrew@andrewshell.org"
        }
    ],
    "repositories": [{
        "type": "composer",
        "url": "https://packages.cadrephp.com"
    }],
    "scripts": {
        "post-install-cmd" : [
            "php bin/assetcopy"
        ],
        "post-update-cmd" : [
            "php bin/assetcopy"
        ]
    },
    "minimum-stability": "stable"
}
